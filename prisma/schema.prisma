generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model complaints {
  id               Int      @id @default(autoincrement())
  title            String   @db.VarChar(255)
  description      String?
  date_of_creation DateTime @db.Date
  category         String?  @db.VarChar(50)
  upvotes          Int?     @default(0)
}
